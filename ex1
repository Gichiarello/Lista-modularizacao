/*saida: diferenca entre os dois numeros inseridos; 
entrada: dois numeros inteiros;
eu usei o "return" para simplificar o calculo.
*/
#include <stdio.h>

int dif(int x, int y){
	return x-y;
}
void main(){
	int x, y, res;
	scanf("%d", &x);
	scanf("%d", &y);
	res = dif(x, y);
	printf("diferenca = %d", res);
	return 0;
}
